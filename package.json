{
  "name": "jg",
  "version": "1.4.2",
  "description": "",
  "main": "server/app.js",
  "scripts": {
    "start": "node server/app.js",
    "nodemon": "nodemon --legacy-watch ./server/app.js",
    "dev": "docker-compose up",
    "dev:detached": "docker-compose up -d",
    "prod": "docker-compose -f docker-compose.yml -f docker-compose-prod.yml up -d",
    "stop": "docker-compose stop",
    "down": "docker-compose down",
    "logs": "docker logs -f jasongallagher",
    "build": "docker exec -it jasongallagher npm run build:script",
    "build:script": "rm -rf dist && NODE_ENV=production webpack --mode production --config ./webpack.production.config.js",
    "build:docker": "npm run build:docker:app && npm run build:docker:db",
    "build:docker:app": "docker build -t jasongallagher:prod -f Dockerfile-app . && docker build --target build -t jasongallagher:dev -f Dockerfile-app .",
    "build:docker:db": "docker build -t jasongallagher_db -f ./database/Dockerfile-db ./database",
    "attach": "docker exec -it jasongallagher /bin/bash",
    "attach:db": "docker exec -it jasongallagher_db bash -c 'psql -U ${POSTGRES_USER} -d ${POSTGRES_DB}'",
    "test": "docker exec -it -e NODE_ENV=test jasongallagher jest",
    "test:coverage": "docker exec -it -e NODE_ENV=test jasongallagher jest --coverage",
    "test:watch": "docker exec -it -e NODE_ENV=test jasongallagher jest --watch",
    "seed": "docker exec -it jasongallagher node_modules/.bin/sequelize db:seed:all",
    "seed:undo": "docker exec -it jasongallagher node_modules/.bin/sequelize db:seed:undo:all",
    "stats": "npm run stats:build && npm run stats:display",
    "stats:build": "docker exec -it jasongallagher bash -c 'webpack --config ./webpack.production.config.js --profile --json > stats.json'",
    "stats:display": "docker exec -it jasongallagher webpack-bundle-analyzer stats.json dist -h 0.0.0.0"
  },
  "author": "Jason Gallagher",
  "license": "RESTRICTED",
